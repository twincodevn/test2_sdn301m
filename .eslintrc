{
  "extends":["nodejs","prettier"],
  "plugins": ["nodejs","prettier"],
  "rules":{
    "prettier/prettier":[
      "warn",
      {
  "arrowParens": "avoid",
  "semi":false,
  "trailingComma": "none",
  "endOfLine": "lf",
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "printWidth": 120,
  "jsxSingleQuote": false
}
    ]
  }
}